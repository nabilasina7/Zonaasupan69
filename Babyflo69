<script>
let adClicked = false;
let currentVideoUrl = '';

// Modified openVideoModal function
function openVideoModal(videoUrl) {
    currentVideoUrl = videoUrl;
    document.getElementById('adPromptModal').style.display = 'flex';
    document.body.style.overflow = 'hidden';
}

function checkAdClick() {
    if (!adClicked) {
        alert('Harap klik link iklan terlebih dahulu sebelum melanjutkan');
        return;
    }
    
    // Show loading before video
    const modalContent = document.getElementById('videoModal').querySelector('.modal-body');
    modalContent.innerHTML = `
        <div style="padding:40px;text-align:center;color:#fff;">
            <div style="border:4px solid #333;border-top:4px solid #4285f4;border-radius:50%;width:40px;height:40px;animation:spin 1s linear infinite;margin:20px auto;"></div>
            <h4 style="margin-bottom:10px;">Mempersiapkan Video...</h4>
        </div>
    `;
    
    document.getElementById('adPromptModal').style.display = 'none';
    document.getElementById('videoModal').style.display = 'flex';
    
    // Load ad script and video after 2 seconds
    setTimeout(() => {
        // Load your ad script
        const adScript = document.createElement('script');
        adScript.type = 'text/javascript';
        adScript.text = `
            atOptions = {
                'key' : '68a000502f8b475d8457bac54ac44875',
                'format' : 'iframe',
                'height' : 50,
                'width' : 320,
                'params' : {}
            };
        `;
        document.body.appendChild(adScript);
        
        const invokeScript = document.createElement('script');
        invokeScript.type = 'text/javascript';
        invokeScript.src = '//revealedcarry.com/68a000502f8b475d8457bac54ac44875/invoke.js';
        document.body.appendChild(invokeScript);
        
        // Show video
        modalContent.innerHTML = `
            <button onclick="closeVideo()" style="position:absolute;top:15px;right:15px;background:rgba(255,255,255,0.2);color:#fff;border:none;width:30px;height:30px;border-radius:50%;font-size:16px;cursor:pointer;z-index:10;display:flex;align-items:center;justify-content:center;">√ó</button>
            <div style="position:relative;padding-bottom:56.25%;height:0;overflow:hidden;border-radius:8px;">
                <iframe src="${currentVideoUrl}" style="position:absolute;top:0;left:0;width:100%;height:100%;border:none;" allowfullscreen></iframe>
            </div>
        `;
    }, 2000);
}

function closeVideo() {
    document.getElementById('videoModal').style.display = 'none';
    document.body.style.overflow = 'auto';
    adClicked = false; // Reset for next time
}

// Track when user clicks the ad link
document.getElementById('adLink').addEventListener('click', function() {
    adClicked = true;
    
    // Show countdown reminder
    let seconds = 5;
    const countdownEl = document.getElementById('countdown');
    countdownEl.textContent = `Kembali dalam ${seconds} detik...`;
    
    const timer = setInterval(() => {
        seconds--;
        countdownEl.textContent = `Kembali dalam ${seconds} detik...`;
        if (seconds <= 0) {
            clearInterval(timer);
            countdownEl.textContent = 'Klik tombol "Saya Sudah Klik Iklan" untuk melanjutkan';
        }
    }, 1000);
});

// Add CSS styles
const style = document.createElement('style');
style.textContent = `
    @keyframes spin {
        0% { transform: rotate(0deg); }
        100% { transform: rotate(360deg); }
    }
    .sidebar-item {
        cursor: pointer;
        transition: transform 0.2s;
    }
    .sidebar-item:hover {
        transform: scale(1.02);
    }
    #adPromptModal a:hover {
        background: #3367d6 !important;
        transform: translateY(-2px);
    }
    #adPromptModal button:hover {
        background: #2d9249 !important;
        transform: translateY(-2px);
    }
`;
document.head.appendChild(style);
</script>

