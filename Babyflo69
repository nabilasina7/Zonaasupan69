<script>
function openVideoModal(videoUrl) {
    const modal = document.getElementById('videoModal');
    const modalContent = modal.querySelector('.modal-body');
    
    // 1. Tampilkan loading screen pertama
    modalContent.innerHTML = `
        <div class="ad-loading-screen" style="
            padding: 30px;
            text-align: center;
            background: white;
            border-radius: 8px;
            max-width: 500px;
            margin: 0 auto;
        ">
            <div class="loader" style="
                border: 4px solid #f3f3f3;
                border-top: 4px solid #4285F4;
                border-radius: 50%;
                width: 40px;
                height: 40px;
                animation: spin 1s linear infinite;
                margin: 20px auto;
            "></div>
            <h4 style="color: #333; margin-bottom: 10px;">Sedang memuat konten...</h4>
            <p style="color: #666; font-size: 14px;">Video akan dimulai setelah iklan selesai</p>
            <div class="countdown-bar" style="
                background: #f1f1f1;
                height: 6px;
                border-radius: 3px;
                margin: 25px 0;
                overflow: hidden;
            ">
                <div class="progress" style="
                    height: 100%;
                    width: 0%;
                    background: #4285F4;
                    transition: width 9s linear;
                "></div>
            </div>
        </div>
    `;
    
    // 2. Tampilkan modal
    modal.style.display = 'block';
    document.body.style.overflow = 'hidden';
    
    // 3. Mulai progress bar
    setTimeout(() => {
        modalContent.querySelector('.progress').style.width = '100%';
    }, 50);
    
    // 4. Load script iklan (versi baru)
    const adContainer = document.createElement('div');
    adContainer.id = 'adContainer';
    modalContent.appendChild(adContainer);
    
    const adScript = document.createElement('script');
    adScript.src = '//revealedcarry.com/a4/3b/3d/a43b3d4f0ccf825ccedfe7758f91518c.js';
    document.body.appendChild(adScript);
    
    // 5. Setelah 9 detik, ganti dengan video
    setTimeout(() => {
        modalContent.innerHTML = `
            <video id="modalVideoPlayer" controls autoplay style="width:100%; border-radius: 8px;"></video>
            <button class="close-modal" style="
                position: absolute;
                top: 15px;
                right: 15px;
                background: rgba(0,0,0,0.5);
                color: white;
                border: none;
                width: 30px;
                height: 30px;
                border-radius: 50%;
                font-size: 16px;
                cursor: pointer;
            ">√ó</button>
        `;
        
        const player = document.getElementById('modalVideoPlayer');
        player.src = encodeURI(videoUrl);
        
        // Handle close button
        modal.querySelector('.close-modal').addEventListener('click', () => {
            modal.style.display = 'none';
            document.body.style.overflow = 'auto';
            player.pause();
        });
        
        // 6. Popunder setelah 9 detik video berjalan
        setTimeout(() => {
            const popScript = document.createElement('script');
            popScript.src = '//pl27861701.effectivegatecpm.com/a3/fa/a0/a3faa0d451dcb0e04a5d51b040f70cee.js';
            document.body.appendChild(popScript);
        }, 9000);
    }, 9000);
}

// CSS untuk animasi
const style = document.createElement('style');
style.textContent = `
    @keyframes spin {
        0% { transform: rotate(0deg); }
        100% { transform: rotate(360deg); }
    }
    #videoModal {
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: rgba(0,0,0,0.8);
        z-index: 9999;
        display: none;
    }
    .modal-body {
        position: relative;
        background: white;
        margin: 5% auto;
        padding: 20px;
        width: 80%;
        max-width: 800px;
        border-radius: 10px;
    }
`;
document.head.appendChild(style);

    function closeVideoModal() {
        const modal = document.getElementById('videoModal');
        const player = document.getElementById('modalVideoPlayer');
        
        // Pause video and close
        player.src = '';
        modal.classList.remove('show');
        document.body.style.overflow = '';
    }

    // Improved overlay click detection
    document.addEventListener('click', (e) => {
        if(e.target.classList.contains('zona-modal')) {
            closeVideoModal();
        }
    });
</script>
<script>

    // Fungsi untuk memfilter video berdasarkan input pencarian
    function searchVideos() {
        const searchInput = document.getElementById('search-input');
        const searchTerm = searchInput.value.toLowerCase();
        const sidebarItems = document.querySelectorAll('.sidebar-item');

        sidebarItems.forEach(item => {
            const title = item.querySelector('.sidebar-title').textContent.toLowerCase();
            const channel = item.querySelector('.sidebar-channel').textContent.toLowerCase();
            
            // Cari baik di judul maupun channel
            if (title.includes(searchTerm) || channel.includes(searchTerm)) {
                item.style.display = 'flex';
            } else {
                item.style.display = 'none';
            }
        });
    }

    // Inisialisasi event listeners untuk pencarian
    document.addEventListener('DOMContentLoaded', function() {
        // Tombol search
        document.getElementById('search-btn').addEventListener('click', searchVideos);
        
        // Input search (enter key)
        document.getElementById('search-input').addEventListener('keyup', function(e) {
            if (e.key === 'Enter') {
                searchVideos();
            }
        });
    });


    
   function openSharePopup() {
    document.getElementById('overlay').style.display = 'block';
    document.getElementById('share-popup').style.display = 'block';
}

function closeSharePopup() {
    document.getElementById('overlay').style.display = 'none';
    document.getElementById('share-popup').style.display = 'none';
}


    // Fungsi tambahan untuk berbagi
    function shareOnWhatsApp() {
         const url = encodeURIComponent(window.location.href);
        window.open('ttps://www.media69digitalstore.my.id/vip' + url, '_blank');
    }

    function shareOnFacebook() {
        const url = encodeURIComponent(window.location.href);
        window.open('https://www.facebook.com/sharer/sharer.php?u=' + url, '_blank');
    }

    function shareOnTelegram() {
        const message = encodeURIComponent('Cek disini, gudangnya asupan all in one yang Aman, VIP dan Esklusif!');
        const url = 'https://t.me/?text=' + message;
        window.open(url, '_blank');
    }
    
// Simple Slider Functionality
document.addEventListener('DOMContentLoaded', function() {
    const track = document.querySelector('.banner-track');
    const slides = document.querySelectorAll('.banner-slide');
    const dots = document.querySelectorAll('.dot');
    let currentIndex = 0;
    
    // Initialize first slide
    slides[0].classList.add('active');
    
    // Auto-rotate slides every 5 seconds
    setInterval(() => {
        currentIndex = (currentIndex + 1) % slides.length;
        updateSlider();
    }, 3900);
    
    // Dot click events
    dots.forEach((dot, index) => {
        dot.addEventListener('click', () => {
            currentIndex = index;
            updateSlider();
        });
    });
    
    function updateSlider() {
        // Update track position
        track.style.transform = `translateX(-${currentIndex * 100}%)`;
        
        // Update slide active states
        slides.forEach((slide, i) => {
            slide.classList.toggle('active', i === currentIndex);
        });
        
        // Update dot states
        dots.forEach((dot, i) => {
            dot.classList.toggle('active', i === currentIndex);
        });
    }
});
// Slider Functionality
document.addEventListener('DOMContentLoaded', function() {
    const track = document.querySelector('.slider-container');
    const slides = document.querySelectorAll('.slide');
    const dots = document.querySelectorAll('.dot');
    let currentIndex = 0;
    
    // Auto-rotate every 3s
    const sliderInterval = setInterval(nextSlide, 3096);
    
    function nextSlide() {
        currentIndex = (currentIndex + 1) % slides.length;
        updateSlider();
    }
    
    // Dot controls
    dots.forEach((dot, index) => {
        dot.addEventListener('click', () => {
            currentIndex = index;
            updateSlider();
            resetInterval();
        });
    });
    
    function updateSlider() {
        track.style.transform = `translateX(-${currentIndex * 100}%)`;
        
        dots.forEach((dot, i) => {
            dot.classList.toggle('active', i === currentIndex);
        });
    }
    
    function resetInterval() {
        clearInterval(sliderInterval);
        sliderInterval = setInterval(nextSlide, 3096);
    }
    
    // Pause on hover
    track.addEventListener('mouseenter', () => clearInterval(sliderInterval));
    track.addEventListener('mouseleave', () => resetInterval());
});
function showAdAndCountdown() {
    const downloadBtn = document.getElementById("downloadBtn");
    const adContainer = document.getElementById("adContainer");
    const timerElement = document.getElementById("timer");
    
    // 1. Tampilkan iklan
    adContainer.style.display = "block";
    downloadBtn.disabled = true;
    
    // 2. Mulai countdown
    let secondsLeft = 9;
    const countdown = setInterval(() => {
        secondsLeft--;
        timerElement.textContent = secondsLeft;
        
        // 3. Selesai countdown
        if (secondsLeft <= 0) {
            clearInterval(countdown);
            adContainer.style.display = "none";
            downloadBtn.disabled = false;
            downloadVideo(); // Lanjut download
        }
    }, 1000);
}

function downloadVideo() {
    const downloadUrl = "https://filemoon.to/download/hy2j66xcworq"; 
    const link = document.createElement('a');
    link.href = downloadUrl;
    link.setAttribute('download', '');
    link.click();
}
</script>
<script>
    // Simulasi fungsi kontrol
    function togglePlay() {
        console.log("Play/Pause triggered");
    }

    function toggleFullscreen() {
        const player = document.querySelector('.video-container');
        if (!document.fullscreenElement) {
            player.requestFullscreen().catch(err => {
                console.error(`Error attempting to enable fullscreen: ${err.message}`);
            });
        } else {
            document.exitFullscreen();
        }
    }
</script>

    <script>
        // üî• Fullscreen Guaranteed Work
        function toggleFullscreen() {
            const elem = document.querySelector('.video-wrapper');
            
            if (!document.fullscreenElement) {
                if (elem.requestFullscreen) {
                    elem.requestFullscreen();
                } else if (elem.webkitRequestFullscreen) { /* Safari */
                    elem.webkitRequestFullscreen();
                } else if (elem.msRequestFullscreen) { /* IE11 */
                    elem.msRequestFullscreen();
                }
            } else {
                if (document.exitFullscreen) {
                    document.exitFullscreen();
                }
            }
        }

        // Fallback untuk mobile
        document.addEventListener('fullscreenchange', () => {
            const fsBtn = document.querySelector('.fullscreen-btn');
            fsBtn.textContent = document.fullscreenElement ? "‚õ∂ Exit" : "‚õ∂ Fullscreen";
        });
        
    </script>
    
    
    <script>
(function(){
  // helpers
  const qs = (s, r=document) => r.querySelector(s);
  const qsa = (s, r=document) => Array.from(r.querySelectorAll(s));

  // find elements
  const tabsRoot = document.getElementById('tabsComponent');
  if (!tabsRoot) return console.warn('[tabs] tabsComponent not found');

  const tabButtons = qsa('.tab-btn', tabsRoot);
  const tabPanels = qsa('.tab-panel', tabsRoot);
  const panelSemua = qs('.tab-panel[data-panel="semua"]', tabsRoot);
  const panelAction = qs('.tab-panel[data-panel="action"]', tabsRoot);
  const panelDrama = qs('.tab-panel[data-panel="drama"]', tabsRoot);

  // utility: activate tab by name
  function activateTab(name, focusBtn=false) {
    tabButtons.forEach(btn=>{
      const is = btn.dataset.tab === name;
      btn.classList.toggle('active', is);
      btn.setAttribute('aria-selected', String(is));
      btn.tabIndex = is ? 0 : -1;
    });
    tabPanels.forEach(panel=>{
      panel.classList.toggle('hidden', panel.dataset.panel !== name);
      panel.setAttribute('aria-hidden', String(panel.dataset.panel !== name));
    });
    if (focusBtn) {
      const btn = qs('.tab-btn[data-tab="'+name+'"]', tabsRoot);
      if (btn) btn.focus();
    }
  }

  // keyboard navigation for tabs (Left/Right/Home/End + Enter/Space)
  function onTabKeydown(e) {
    const key = e.key;
    const active = document.activeElement;
    if (!active || !active.classList.contains('tab-btn')) return;
    const idx = tabButtons.indexOf(active);
    if (idx === -1) return;

    if (key === 'ArrowRight' || key === 'ArrowLeft' || key === 'Home' || key === 'End') {
      e.preventDefault();
      let newIdx = idx;
      if (key === 'ArrowRight') newIdx = (idx+1) % tabButtons.length;
      else if (key === 'ArrowLeft') newIdx = (idx-1+tabButtons.length) % tabButtons.length;
      else if (key === 'Home') newIdx = 0;
      else if (key === 'End') newIdx = tabButtons.length - 1;
      tabButtons[newIdx].focus();
    } else if (key === 'Enter' || key === ' ') {
      e.preventDefault();
      activateTab(active.dataset.tab, true);
    }
  }

  // click handler for tabs
  tabButtons.forEach(btn => {
    btn.addEventListener('click', ()=> activateTab(btn.dataset.tab, false));
    btn.addEventListener('keydown', onTabKeydown);
  });

  // render: ensure panels exist
  if (!panelSemua) {
    console.warn('[tabs] panel-semua not found');
  }

  // ensure each panel has at least one video: clone from semua
  function ensurePanelsHaveOneVideo() {
    const semuaItems = qsa('.tab-panel[data-panel="semua"] .sidebar-item', tabsRoot);
    if (!semuaItems || semuaItems.length === 0) return;

    // helper find by keywords in title/src
    function findByKeywords(keywords) {
      const lower = keywords.map(s=>s.toLowerCase());
      for (const it of semuaItems) {
        const title = (it.querySelector('.sidebar-title')?.textContent || '').toLowerCase();
        const src = (it.getAttribute && it.getAttribute('onclick') || '').toLowerCase();
        // check title or onclick url
        if (lower.some(k => title.includes(k) || src.includes(k))) return it;
      }
      return null;
    }

    // for action: try keywords then fallback to index 1
    let actionSource = findByKeywords(['action','duel','kolab','power','adu','service','duel','fight','action']) || semuaItems[1] || semuaItems[0];
    let dramaSource = findByKeywords(['drama','cinta','cerita','interview','drama','romance']) || semuaItems[2] || semuaItems[0];

    // clone and append if panels are empty
    if (panelAction && qsa('.sidebar-item', panelAction).length === 0) {
      panelAction.appendChild(actionSource.cloneNode(true));
    }
    if (panelDrama && qsa('.sidebar-item', panelDrama).length === 0) {
      panelDrama.appendChild(dramaSource.cloneNode(true));
    }
  }

  // Delegated click for items (open modal)
  function onItemClick(e) {
    const item = e.target.closest('.sidebar-item');
    if (!item) return;
    // get URL from onclick attribute or data-src
    // support onclick="openVideoModal('URL')" pattern
    let url = null;
    if (item.dataset && item.dataset.src) url = item.dataset.src;
    // parse onclick if present
    if (!url && item.getAttribute('onclick')) {
      const onclick = item.getAttribute('onclick');
      const m = onclick.match(/openVideoModal\(['"]([^'"]+)['"]\)/i);
      if (m) url = m[1];
    }
    const title = item.querySelector('.sidebar-title')?.textContent || '';
    if (!url) { console.warn('No url found for item'); return; }

    // prefer existing functions
    if (typeof window.openVideoModal === 'function') {
      try { window.openVideoModal(url, title); return; } catch(e){ /* fallthrough */ }
    }
    if (typeof window.showStyledModal === 'function') {
      try { window.showStyledModal(url, title); return; } catch(e){ /* fallthrough */ }
    }
    // default: open in new tab
    window.open(url, '_blank', 'noopener,noreferrer');
  }

  // keyboard activation for item (Enter/Space)
  function onItemKeydown(e) {
    if ((e.key === 'Enter' || e.key === ' ') && document.activeElement && document.activeElement.classList.contains('sidebar-item')) {
      e.preventDefault();
      document.activeElement.click();
    }
  }

  // attach delegation to each panel container
  tabPanels.forEach(panel => {
    panel.addEventListener('click', onItemClick);
    panel.addEventListener('keydown', onItemKeydown);
    // make items focusable if not already
    qsa('.sidebar-item', panel).forEach(it => {
      if (!it.hasAttribute('tabindex')) it.setAttribute('tabindex', '0');
    });
  });

  // run initial population and setup
  document.addEventListener('DOMContentLoaded', function(){
    ensurePanelsHaveOneVideo();
    // make dynamic items focusable
    tabPanels.forEach(panel => {
      qsa('.sidebar-item', panel).forEach(it => {
        if (!it.hasAttribute('tabindex')) it.setAttribute('tabindex','0');
      });
    });
    // initial active tab: find .tab-btn.active or default 'semua'
    const initial = tabButtons.find(b=>b.classList.contains('active')) || tabButtons.find(b=>b.dataset.tab==='semua') || tabButtons[0];
    if (initial) activateTab(initial.dataset.tab, false);
  });

  // accessibility: close any open panels when navigating away (optional)
})();
</script>
</body>

</html>

